@Article{orbit_length,
  author       = {Senichev, Y. and Aksentyev, A. and Melnikov, A.},
  date         = {2021},
  journaltitle = {Physics of Atomic Nuclei},
  title        = {Spin Chromaticity of Beam: Orbit Lengthening and Betatron Chromaticity},
  doi          = {10.1134/S1063778821100367},
  issn         = {1562-692X},
  number       = {12},
  pages        = {2014--2017},
  url          = {https://doi.org/10.1134/S1063778821100367},
  volume       = {84},
  abstract     = {One possible method of measuring the electric dipole moment of an elementary particle consists in measuring the average spin precession frequency of a polarized beam. The problem of maximizing the spin coherence time of a bunched beam is reduced to the minimization of the particles’ orbit length dispersion. This is achieved by introducing sextupoles with fields minimizing the chromaticity of the spin frequency. We have found experimentally that minimizing the chromaticity of betatron motion leads to the minimization of the chromaticity of the spin frequency. In this paper we explore this relationship.},
  refid        = {Senichev2021},
}

